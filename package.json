{
  "name": "@alisaitteke/adb-client",
  "version": "1.0.8",
  "description": "Modern TypeScript library for Android Debug Bridge (ADB)",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alisaitteke/adb-client.git"
  },
  "keywords": [
    "adb",
    "android",
    "typescript",
    "debug-bridge",
    "android-debug-bridge",
    "mobile",
    "development"
  ],
  "author": {
    "name": "Ali Sait Teke",
    "email": "alisaitteke@gmail.com",
    "url": "https://github.com/alisaitteke"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alisaitteke/adb-client/issues"
  },
  "homepage": "https://github.com/alisaitteke/adb-client#readme",
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "package.json",
    "package-lock.json",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
